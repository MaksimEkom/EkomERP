<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.ekom.ekomerp.entity.Consumption"
          extends="_local"
          name="consumption-edit">
        <property name="consumableProduct"
                  view="_minimal">
            <property name="code"/>
            <property name="unit"
                      view="_minimal"/>
        </property>
        <property name="product"
                  view="_minimal">
            <property name="code"/>
            <property name="analog"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Laboriousness"
          extends="_local"
          name="laboriousness-edit">
        <property name="operation"
                  view="_minimal">
            <property name="name"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Category"
          extends="_local"
          name="categoryWithParent">
        <property name="parentCategory"
                  view="_minimal">
            <property name="code"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Product"
          extends="_local"
          name="productWithCategoryAndUnitAndLaboriousnessAndConsumption">
        <property name="unit"
                  view="_minimal"/>
        <property name="category"
                  view="_minimal"/>
        <property name="laboriousness"
                  view="_minimal">
            <property name="operation"
                      view="_minimal">
            </property>
            <property name="value"/>
        </property>
        <property name="consumption"
                  view="_minimal">
            <property name="consumableProduct"
                      view="_minimal">
                <property name="unit"
                          view="_minimal"/>
            </property>
            <property name="quantity"/>
        </property>
        <property name="image"
                  view="_minimal"/>
        <property name="productPrice"
                  view="_minimal">
            <property name="price"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Location"
          extends="_local"
          name="locationWithStockman">
        <property name="stockman"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.StockMovement"
          extends="_local"
          name="stockMovement-edit">
        <property name="location"
                  view="_minimal"/>
        <property name="stockMovementLine"
                  view="_minimal">
            <property name="product"
                      view="_minimal">
                <property name="unit"
                          view="_minimal"/>
                <property name="code"/>
            </property>
            <property name="quantity"/>
            <property name="quantityBefore"/>
            <property name="quantityAfter"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.StockMovementLine"
          extends="_local"
          name="stockMovementLine-edit">
        <property name="product"
                  view="_minimal">
            <property name="unit"
                      view="_minimal"/>
            <property name="code"/>
        </property>
        <property name="stockMovement"
                  view="_minimal">
            <property name="location"
                      view="_minimal"/>
            <property name="stockMovementType"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Product"
          extends="_local"
          name="productWithUnit">
        <property name="unit"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.StockMovement"
          extends="_local"
          name="stockMovementWithLocation">
        <property name="location"
                  view="_minimal">
            <property name="stockman"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Inventory"
          extends="_local"
          name="inventory-view">
        <property name="location"
                  view="_local">
            <property name="stockman"
                      view="_minimal"/>
        </property>
        <property name="product"
                  view="_minimal">
            <property name="code"/>
            <property name="unit"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Partner"
          extends="_local"
          name="partnerWithParentView">
        <property name="parent"
                  view="_minimal"/>
        <property name="region"
                  view="_minimal"/>
        <property name="country"
                  view="_minimal"/>
        <property name="image"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.StockMovementLine"
          extends="_local"
          name="stockMovementLine-view">
        <property name="product"
                  view="_minimal">
            <property name="unit"
                      view="_minimal"/>
        </property>
        <property name="stockMovement"
                  view="_minimal">
            <property name="consignment"/>
            <property name="date"/>
            <property name="location"
                      view="_minimal"/>
            <property name="stockMovementType"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Region"
          extends="_local"
          name="regionWithCountryview">
        <property name="country"
                  view="_minimal">
            <property name="code"/>
            <property name="name"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.PurchaseOrder"
          extends="_local"
          name="purchaseOrder-view">
        <property name="vendor"
                  view="_minimal"/>
        <property name="purchaseOrderLine"
                  view="_minimal">
            <property name="product"
                      view="_minimal">
                <property name="unit"
                          view="_minimal"/>
            </property>
            <property name="quantity"/>
            <property name="price"/>
            <property name="subtotal"/>
            <property name="tax"/>
            <property name="total"/>
            <property name="purchaseOrder"
                      view="_minimal"/>
        </property>
        <property name="paymentCondition"
                  view="_minimal"/>
        <property name="invoiceFile"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.PurchaseOrderLine"
          extends="_local"
          name="purchaseOrderLine-view">
        <property name="product"
                  view="_minimal">
            <property name="unit"
                      view="_minimal"/>
        </property>
        <property name="purchaseOrder"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.ManufacturingOrderLine"
          extends="_local"
          name="manufacturingOrderLine-view">
        <property name="product"
                  view="_minimal">
            <property name="code"/>
            <property name="unit"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.ManufacturingOrder"
          extends="_local"
          name="manufacturingOrder-view">
        <property name="customer"
                  view="_minimal"/>
        <property name="manufacturingOrderLine"
                  view="manufacturingOrderLine-view"/>
    </view>
    <view class="com.ekom.ekomerp.entity.InvoiceLine"
          extends="_local"
          name="invoiceLine-view">
        <property name="product"
                  view="_minimal">
            <property name="unit"
                      view="_minimal"/>
        </property>
        <property name="invoice"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.Invoice"
          extends="_local"
          name="invoice-view">
        <property name="partner"
                  view="_minimal"/>
        <property name="invoiceLine"
                  view="_minimal">
            <property name="product"
                      view="_minimal">
                <property name="unit"
                          view="_minimal"/>
            </property>
            <property name="quantity"/>
            <property name="price"/>
            <property name="subtotal"/>
            <property name="tax"/>
            <property name="total"/>
            <property name="invoice"
                      view="_minimal"/>
        </property>
        <property name="invoiceFile"
                  view="_minimal"/>
        <property name="payments"
                  view="_minimal">
            <property name="date"/>
            <property name="paymentMethod"/>
            <property name="notes"/>
            <property name="paymentType"/>
            <property name="amount"/>
        </property>
        <property name="paid"/>
        <property name="debt"/>
    </view>
    <view class="com.ekom.ekomerp.entity.Product"
          extends="_local"
          name="productWithConsumption">
        <property name="unit"
                  view="_minimal"/>
        <property name="consumption"
                  view="_minimal">
            <property name="product"
                      view="_minimal"/>
            <property name="consumableProduct"
                      view="_minimal"/>
            <property name="quantity"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Product"
          extends="_minimal"
          name="productWithLaboriousnessAndConsumption">
        <property name="laboriousness"
                  view="_minimal">
            <property name="product"
                      view="_minimal"/>
            <property name="operation"
                      view="_minimal"/>
            <property name="value"/>
        </property>
        <property name="consumption"
                  view="_minimal">
            <property name="product"
                      view="_minimal"/>
            <property name="consumableProduct"
                      view="_minimal"/>
            <property name="quantity"/>
        </property>
        <property name="code"/>
        <property name="unit"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.Product"
          extends="productWithUnit"
          name="productWithUnitAndCategory">
        <property name="category"
                  view="_minimal"/>
        <property name="laboriousness"
                  view="laboriousness-edit">
            <property name="product"
                      view="_minimal"/>
        </property>
        <property name="consumption"
                  view="consumption-edit"/>
        <property name="productPrice"
                  view="_minimal">
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.ProductPrice"
          extends="_local"
          name="productPrice-edit">
        <property name="product"
                  view="_minimal"/>
        <property name="pricingType"
                  view="_minimal">
            <property name="name"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Calculation"
          extends="_local"
          name="calculation-edit">
        <property fetch="BATCH"
                  name="product"
                  view="_minimal">
            <property name="code"/>
            <property name="unit"
                      view="_minimal"/>
            <property name="category"
                      view="_minimal"/>
            <property fetch="BATCH"
                      name="laboriousness"
                      view="_minimal">
                <property name="product"
                          view="_minimal"/>
                <property name="operation"
                          view="_minimal"/>
                <property name="value"/>
            </property>
            <property fetch="BATCH"
                      name="consumption"
                      view="_minimal">
                <property name="product"
                          view="_minimal"/>
                <property name="consumableProduct"
                          view="_minimal"/>
                <property name="quantity"/>
            </property>
        </property>
        <property name="consumptionLine"
                  view="_minimal">
            <property name="material"
                      view="_minimal">
                <property name="code"/>
                <property name="unit"
                          view="_minimal"/>
            </property>
            <property name="quantity"/>
            <property name="price"/>
        </property>
        <property name="laboriousLine"
                  view="_minimal">
            <property name="operation"
                      view="_minimal"/>
            <property name="value"/>
            <property name="price"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.Calculation"
          extends="_local"
          name="calculation-view">
        <property name="product"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.Payment"
          extends="_local"
          name="payment-view">
        <property name="invoice"
                  view="_minimal">
            <property name="type"/>
        </property>
    </view>
    <view class="com.ekom.ekomerp.entity.DoorOrder"
          extends="_local"
          name="doorOrder-view"
          systemProperties="true">
        <property name="partner"
                  view="_minimal">
            <property name="phone"/>
            <property name="street"/>
            <property name="city"/>
        </property>
        <property name="door"
                  view="_minimal"/>
    </view>
    <view class="com.ekom.ekomerp.entity.DoorOrder"
          extends="doorOrder-view"
          name="doorOrder-edit"/>
    <view class="com.ekom.ekomerp.entity.Gauging"
          extends="_local"
          name="gauging-view">
    </view>
</views>
