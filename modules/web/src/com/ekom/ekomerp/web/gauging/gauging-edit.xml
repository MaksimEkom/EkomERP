<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.ekom.ekomerp.web.gauging.GaugingEdit"
        datasource="gaugingDs"
        focusComponent="fieldGroup"
        messagesPack="com.ekom.ekomerp.web.gauging">
    <dsContext>
        <datasource id="gaugingDs"
                    class="com.ekom.ekomerp.entity.Gauging"
                    view="gauging-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="gaugingDs">
            <column width="250px">
                <field property="doorOrderNumber"/>
                <field property="width"/>
                <field property="height"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
